{
  "pages": [
    "pages/home/index",
    "pages/explore/index",
    "pages/my/index",
    "pages/personality-test/index"
  ],
  "usingComponents": {
    "t-toast": "tdesign-miniprogram/toast/toast"
  },
  "subpackages": [
    {
      "root": "pages/search",
      "name": "search",
      "pages": ["index"]
    },
    {
      "root": "pages/my/favorites",
      "name": "favorites",
      "pages": ["index"]
    },
    {
      "root": "pages/my/test",
      "name": "test",
      "pages": ["index"]
    },
    {
      "root": "pages/login",
      "name": "login",
      "pages": ["login"]
    },
    {
      "root": "pages/setting",
      "name": "setting",
      "pages": ["index"]
    },
    {
      "root": "pages/fish",
      "name": "fish",
      "pages": ["detail/index"]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "宠物鱼",
    "navigationBarTextStyle": "black"
  },
  "tabBar": {
    "custom": true,
    "color": "#000000",
    "selectedColor": "#000000",
    "backgroundColor": "#000000",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/explore/index",
        "text": "发现"
      },
      {
        "pagePath": "pages/my/index",
        "text": "我的"
      }
    ]
  },
  "resolveAlias": {
    "~/*": "/*"
  },
  "sitemapLocation": "sitemap.json"
}
